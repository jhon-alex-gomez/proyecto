<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Título de la página</title>
  <script src="https://cdn.anychart.com/releases/8.11.0/js/anychart-core.min.js"></script>
  <script src="https://cdn.anychart.com/releases/8.11.0/js/anychart-cartesian.min.js"></script>
  <style>
    body, html {
      height: 100%;
      margin: 0;
      padding: 0;
    }
    
    #container, #container2 {
      width: 80%;
      height: 80%;
      margin: auto;
      background-color: white;
      margin-bottom: 60px ;
    }
  </style>
</head>
<body>
  <div class="wrapper">
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <h2>Graficas de temperatura y humedad</h2>
      

      <div id="container"></div>
      <div id="container2"></div>

      <script>
        // create data for the first chart
        var data1 = [
          ["01-junio", 26],
          ["2-junio", 30],
          ["3-junio", 33],
          ["4-junio", 32],
          ["5-junio", 30],
          ["6-junio", 31],
          ["7-junio", 30],
          ["8-junio", 27]
        ];
      
        // create a chart for the first data
        var chart1 = anychart.line();

        // set the title for the first chart
        chart1.title("Temperatura °C");

        // create a line series and set the data
        var series1 = chart1.line(data1);

        // set the container id
        chart1.container("container");

        // initiate drawing the chart
        chart1.draw();

        // create data for the second chart
        var data2 = [
          ["01-junio", 80],
          ["2-junio", 82],
          ["3-junio", 85],
          ["4-junio", 83],
          ["5-junio", 82],
          ["6-junio", 80],
          ["7-junio", 81],
          ["8-junio", 86]
        ];

        // create a chart for the second data
        var chart2 = anychart.line();

        // set the title for the second chart
        chart2.title("Humedad %");

        // create a line series and set the data
        var series2 = chart2.line(data2);

        // set the container id
        chart2.container("container2");

        // initiate drawing the chart
        chart2.draw();
      </script>
    </div>
  </div>
</body>
</html>

